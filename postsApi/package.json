{
  "name": "nestjs-api",
  "version": "0.0.4",
  "description": "",
  "author": "mirzhalalov.kg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "../node_modules/.bin/rimraf dist",
    "build": "../node_modules/.bin/nest build",
    "format": "../node_modules/.bin/prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "../node_modules/.bin/nest start",
    "start:dev": "../node_modules/.bin/nodemon --config nodemon.json",
    "start:debug": "../node_modules/.bin/nest build && ts-node dist/main",
    "start:prod": "../node_modules/.bin/ts-node dist/main",
    "lint": "../node_modules/.bin/eslint \\\"{src,apps,libs,test}/**/*.ts\\\" --fix",

    "test": "../node_modules/.bin/jest",
    "test:watch": "../node_modules/.bin/jest --watch",
    "test:cov": "../node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "test:e2e": "../node_modules/.bin/jest --config ./test/jest-e2e.json",

    "typeorm": "../node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -d src/config/config.service.ts ",
    "typeorm:migration:run": "npm run typeorm -- migration:run -d src/config/config.service.ts ",
    "migrations:run": "npm run typeorm:migration:generate -- src/migration/my_init && npm run typeorm:migration:run"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
